<AnchorPane prefHeight="267.0" prefWidth="317.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CatalogFrameController">
   <children>
      <ImageView fx:id="flowerImage" fitHeight="193.0" fitWidth="141.0" layoutX="60.0" layoutY="206.0" pickOnBounds="true" preserveRatio="true" rotate="-90.0">
         <image>
            <Image url="@Images/Flower.jpeg" />
         </image>
      </ImageView>
      <Button id="btnExit" fx:id="btnExit" layoutX="238.0" layoutY="251.0" mnemonicParsing="false" onAction="#getExitBtn1" text="Exit" />
      <Button id="btnProductInfo" fx:id="btnProductInfo" layoutX="31.0" layoutY="180.0" mnemonicParsing="false" onAction="#ProductInfo" text="Bouquet Info" />
      <Label id="lstOfProduct" layoutX="31.0" layoutY="76.0" text="List of Bouquet:" />
      <Text layoutX="68.0" layoutY="50.0" stroke="PURPLE" text="Catalog Edit">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
      </Text>
      <ComboBox fx:id="productBox" layoutX="31.0" layoutY="105.0" onAction="#getChosenProduct" prefWidth="150.0" />
   </children>
</AnchorPane>
